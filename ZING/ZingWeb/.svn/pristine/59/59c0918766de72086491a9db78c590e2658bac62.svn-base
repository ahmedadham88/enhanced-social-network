body {
	margin-left: 0px;
	margin-top: 0px;
	margin-right: 0px;
	margin-bottom: 0px;
	background-image: url();
	background-repeat: no-repeat;
	background-color: #C4A2A2;
}

.welcome {
	font-size: 12px;
	color: #CC6633;
	font-weight: bold;
	font-family: Verdana, Arial, Helvetica, sans-serif;
}

.searchbox {
background:#CCCC99;
font:Verdana, Arial, Helvetica, sans-serif;
font-size:14px;
color:#FFFFFF;
font-weight:bold;
}

.regbox {
background:#999966;
border:thin;
border-color:#99CCFF;
font:Verdana, Arial, Helvetica, sans-serif;
font-size:14px;
color:#0099FF;
font-weight:bold;
}

.loginbox {
background:#FFFFCC;
border:double;
border-color:#660066;
font:Verdana, Arial, Helvetica, sans-serif;
font-size:14px;
color:#99CCCC;
font-weight:bold;
}

.logintext {
	font-size:14px;
	color:#99CC00;
	font-weight:bold;
	font-family: Verdana, Arial, Helvetica, sans-serif;
}
.toplink {
	font-size: 12px;
	color: #FFFFFF;
	font-style: normal;
	font-weight: bolder;
}
a {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-style: normal;
	font-size:12px;
	text-decoration: none;
}
img {
cursor:pointer;
}
h2 {
color:#FFCC66;
}

h3 {
color:#FFCC66;
}

a:hover {
/*	font-size:14px;*/
	text-decoration: underline;
	/*color:#FF0066;*/
	font-style: normal;

}
marquee {
	font-family: Courier New (monospace);
	color: #FFFFFF;
	border: 2px dotted #FFFFFF;
	background-color:#BF6D6F;
}
.marqlink{
color:#FFCC00;
}

.SIGNIN {
	color:#660033;
	font-weight: bold;
	font-style: normal;
	font-size: 24px;
}

td {
	font-family: Verdana, Arial, Helvetica, sans-serif;
	font-size: 12px;
	font-style: normal;
}
