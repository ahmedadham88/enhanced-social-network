<!--  gaurav -->
	<navigation-rule>
		<navigation-case>
			<from-action>#{friendProfileHandler.getFriendProfile}</from-action>
			<from-outcome>lnkFriendProfile</from-outcome>
			<to-view-id>/pages/friendsProfile.jspx</to-view-id>
